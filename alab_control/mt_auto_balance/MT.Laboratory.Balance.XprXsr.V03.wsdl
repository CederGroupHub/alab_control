          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="DischargeRequest">
        <xsd:annotation>
          <xsd:documentation>Contains the parameters for the IFeederService.Discharge request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="1" name="SessionId" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The session ID.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="DischargeResponse">
        <xsd:annotation>
          <xsd:documentation>The response to the IFeederService.Discharge request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="Outcome" type="tns:Outcome">
              <xsd:annotation>
                <xsd:documentation>The outcome of the request.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="ErrorMessage" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The optional error message in case of an error outcome.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="FeederAttached" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>Returns 'true' if a feeder is attached.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="StopFeederRequest">
        <xsd:annotation>
          <xsd:documentation>Contains the parameters for the IFeederService.StopFeeder request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="1" name="SessionId" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The session ID.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="StopFeederResponse">
        <xsd:annotation>
          <xsd:documentation>The response to the IFeederService.StopFeeder request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="Outcome" type="tns:Outcome">
              <xsd:annotation>
                <xsd:documentation>The outcome of the request.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="ErrorMessage" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The optional error message in case of an error outcome.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="FeederAttached" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>Returns 'true' if a feeder is attached.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="GetPositionRequest">
        <xsd:annotation>
          <xsd:documentation>Contains the parameters for the IDraftShieldsService.GetPosition request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="1" name="SessionId" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The session ID.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="DraftShieldIds" type="tns:ArrayOfDraftShieldIdentifier">
              <xsd:annotation>
                <xsd:documentation>Gets or sets one or multiple draft shield IDs.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:complexType name="ArrayOfDraftShieldIdentifier">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="DraftShieldIdentifier" type="tns:DraftShieldIdentifier" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="DraftShieldIdentifier">
        <xsd:annotation>
          <xsd:documentation>Draft shield identifiers.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="LeftOuter">
            <xsd:annotation>
              <xsd:documentation>Left outer door.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="RightOuter">
            <xsd:annotation>
              <xsd:documentation>Right outer door.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="LeftInner">
            <xsd:annotation>
              <xsd:documentation>Left inner door.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="RightInner">
            <xsd:annotation>
              <xsd:documentation>Right inner door.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Top">
            <xsd:annotation>
              <xsd:documentation>Top door.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Radial">
            <xsd:annotation>
              <xsd:documentation>Radial door.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Flap">
            <xsd:annotation>
              <xsd:documentation>Flap door.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:element name="GetPositionResponse">
        <xsd:annotation>
          <xsd:documentation>The response to the IDraftShieldsService.GetPosition request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="Outcome" type="tns:Outcome">
              <xsd:annotation>
                <xsd:documentation>The outcome of the request.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="ErrorMessage" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The optional error message in case of an error outcome.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="DraftShieldsInformation" type="tns:ArrayOfDraftShieldInformation">
              <xsd:annotation>
                <xsd:documentation>Gets or sets the get draft shield information.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:complexType name="ArrayOfDraftShieldInformation">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="DraftShieldInformation" nillable="true" type="tns:DraftShieldInformation" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:complexType name="DraftShieldInformation">
        <xsd:annotation>
          <xsd:documentation>Draft shield information, containing a description and the position of the draft shield.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent mixed="false">
          <xsd:extension base="tns:DraftShieldPosition">
            <xsd:sequence>
              <xsd:element minOccurs="0" maxOccurs="1" name="Description" type="xsd:string">
                <xsd:annotation>
                  <xsd:documentation>The draft shield description, such as 'top door'.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="1" maxOccurs="1" name="PositionDeterminationOutcome" type="tns:PositionDeterminationOutcome">
                <xsd:annotation>
                  <xsd:documentation>Gets or sets the outcome of the position determination of the draft shield. Note: For radial doors with intermediate PositionDeterminationOutcome, the position is not correct.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="DraftShieldPosition">
        <xsd:annotation>
          <xsd:documentation>Gets or sets the data for the position of a draft shield.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="1" maxOccurs="1" name="DraftShieldId" type="tns:DraftShieldIdentifier">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the draft shield ID.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="1" maxOccurs="1" name="OpeningWidth" type="xsd:int">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the current opening width. Radial door: 0 is closed, 100 is fully opened. Linear door: 0...100</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="1" maxOccurs="1" name="OpeningSide" nillable="true" type="tns:OpeningSide">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the opening side. For doors which can open to the left and right side, the door opening side is listed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:simpleType name="OpeningSide">
        <xsd:annotation>
          <xsd:documentation>The opening side of the draft shield.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="OpenRight">
            <xsd:annotation>
              <xsd:documentation>The right draft shield is open.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="OpenLeft">
            <xsd:annotation>
              <xsd:documentation>The left draft shield is open.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:simpleType name="PositionDeterminationOutcome">
        <xsd:annotation>
          <xsd:documentation>The outcome of the determination of the draft shield position.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Success">
            <xsd:annotation>
              <xsd:documentation>The draft shield was successfully determined.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Intermediate">
            <xsd:annotation>
              <xsd:documentation>The draft shield is in a moving state. Note: This state occurs only for radial draft shields.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Error">
            <xsd:annotation>
              <xsd:documentation>The draft shield is blocked or in error state.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:element name="SetPositionRequest">
        <xsd:annotation>
          <xsd:documentation>Contains the parameters for the IDraftShieldsService.SetPosition request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="1" name="SessionId" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The session ID.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="DraftShieldsPositions" type="tns:ArrayOfDraftShieldPosition">
              <xsd:annotation>
                <xsd:documentation>One or multiple target positions of the draft shields.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:complexType name="ArrayOfDraftShieldPosition">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="DraftShieldPosition" nillable="true" type="tns:DraftShieldPosition" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="SetPositionResponse">
        <xsd:annotation>
          <xsd:documentation>The response to the IDraftShieldsService.SetPosition request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="Outcome" type="tns:Outcome">
              <xsd:annotation>
                <xsd:documentation>The outcome of the request.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="ErrorMessage" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The optional error message in case of an error outcome.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ReadDosingHeadRequest">
        <xsd:annotation>
          <xsd:documentation>The parameters for the IDosingAutomationService.ReadDosingHead request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="1" name="SessionId" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The session ID.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ReadDosingHeadResponse">
        <xsd:annotation>
          <xsd:documentation>The response to the IDosingAutomationService.ReadDosingHead request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="Outcome" type="tns:Outcome">
              <xsd:annotation>
                <xsd:documentation>The outcome of the request.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="ErrorMessage" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The optional error message in case of an error outcome.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="HeadType" type="tns:DosingHeadType">
              <xsd:annotation>
                <xsd:documentation>Gets or sets the dosing head type.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="HeadTypeName" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Gets or sets the dosing head type identifier.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="HeadId" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Gets or sets the dosing head ID.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="DosingHeadInfo" type="tns:DosingHeadInfo">
              <xsd:annotation>
                <xsd:documentation>Gets or sets the dosing head info.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:simpleType name="DosingHeadType">
        <xsd:annotation>
          <xsd:documentation>The dosing head types.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="Powder">
            <xsd:annotation>
              <xsd:documentation>The powder head type.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
          <xsd:enumeration value="Liquid">
            <xsd:annotation>
              <xsd:documentation>The liquid head type.</xsd:documentation>
            </xsd:annotation>
          </xsd:enumeration>
        </xsd:restriction>
      </xsd:simpleType>
      <xsd:complexType name="DosingHeadInfo">
        <xsd:annotation>
          <xsd:documentation>The common info provided in the IDosingAutomationService.ReadDosingHead request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent mixed="false">
          <xsd:extension base="tns:EditableDosingHeadInfo">
            <xsd:sequence>
              <xsd:element minOccurs="0" maxOccurs="1" name="RemainingQuantity" type="tns:ValueWithUnit">
                <xsd:annotation>
                  <xsd:documentation>Gets or sets the remaining quantity. PowderDosing: in mg: 0.01 to 999'999 mg with 2 decimal places or disabled. Liquid Dosing: in g: 0.01 to 9'999 g with 2 decimal places or disabled.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="1" maxOccurs="1" name="NumberOfDosages" nillable="true" type="xsd:unsignedInt">
                <xsd:annotation>
                  <xsd:documentation>Gets or sets the total number of dosages.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="1" maxOccurs="1" name="RemainingDosages" nillable="true" type="xsd:unsignedInt">
                <xsd:annotation>
                  <xsd:documentation>Gets or sets the remaining dosages: dosing Limit XPR - Number of Dosages Total (if not disabled, &gt;0).</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:extension>
        </xsd:complexContent>
      </xsd:complexType>
      <xsd:complexType name="EditableDosingHeadInfo">
        <xsd:annotation>
          <xsd:documentation>The dosing head info provided by the IDosingAutomationService.ReadDosingHead request.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="1" name="SubstanceName" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the substance name (0 to 20 characters).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="LotId" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the lot id (0 to 15 characters).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="1" maxOccurs="1" name="FillingDate" nillable="true" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the filling date or disabled. ISO 8601 format without time zone information 'Year-Month-DayT00:00:00'. Empty to disable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="1" maxOccurs="1" name="ExpiryDate" nillable="true" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the expiry date or disabled. ISO 8601 format without time zone information 'Year-Month-DayT00:00:00'. Empty to disable.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="Id1Label" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the ID 1 label (0 to 10 characters).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="Id1Value" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the ID 1 value (0 to 15 characters).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="Id2Label" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the ID 2 label (0 to 10 characters).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="Id2Value" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the ID 2 value (0 to 15 characters).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="Id3Label" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the ID 3 label (0 to 10 characters).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="Id3Value" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the ID 3 value (0 to 15 characters).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="MolarMass" type="tns:ValueWithUnit">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the molar mass in g/mol: empty or 1 to 10'000, max 5 decimal places.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="Purity" type="tns:ValueWithUnit">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the purity in %: empty or 0.001 to 100, max 3 decimal places.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="FilledUpQuantity" type="tns:ValueWithUnit">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the filled up quantity. PowderDosing: in mg: 0.01 to 999'999 mg with 2 decimal places or disabled. Liquid Dosing: in g: 0.01 to 9'999 g with 2 decimal places or disabled.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="1" maxOccurs="1" name="TappingWhileDosing" nillable="true" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the tapping while dosing on / off.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="1" maxOccurs="1" name="TappingBeforeDosing" nillable="true" type="xsd:boolean">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the tapping before dosing on / off.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="1" maxOccurs="1" name="DosingLimit" nillable="true" type="xsd:unsignedInt">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the dosing limit.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="DensityOfLiquid" type="tns:ValueWithUnit">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the density of liquid: empty or 0.01 to 10 in g/ml with max 6 decimal places.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" maxOccurs="1" name="PumpPressure" type="tns:ValueWithUnit">
            <xsd:annotation>
              <xsd:documentation>Gets or sets the pump pressure: 0.3 to 0.5 bar in steps of 0.1 bar, Default: 0.5 bar.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="WriteDosingHeadRequest">
        <xsd:annotation>
          <xsd:documentation>The parameters for the IDosingAutomationService.WriteDosingHead request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="1" name="SessionId" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The session ID.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" maxOccurs="1" name="HeadType" type="tns:DosingHeadType">
              <xsd:annotation>
                <xsd:documentation>Gets or sets the dosing head type.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="HeadId" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Gets or sets the dosing head ID.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="DosingHeadInfo" type="tns:EditableDosingHeadInfo">
              <xsd:annotation>
                <xsd:documentation>Gets or sets the editable dosing head info.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="WriteDosingHeadResponse">
        <xsd:annotation>
          <xsd:documentation>The response to the IDosingAutomationService.WriteDosingHead request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="1" name="Outcome" type="tns:Outcome">
              <xsd:annotation>
                <xsd:documentation>The outcome of the request.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="ErrorMessage" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The optional error message in case of an error outcome.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="StartExecuteDosingJobListAsyncRequest">
        <xsd:annotation>
          <xsd:documentation>The parameters for the IDosingAutomationService.StartExecuteDosingJobListAsync request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="1" name="SessionId" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>The session ID.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="1" name="DosingJobList" type="tns:ArrayOfDosingJob">
              <xsd:annotation>
                <xsd:documentation>The dosing job list to execute.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:complexType name="ArrayOfDosingJob">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" name="DosingJob" nillable="true" type="tns:DosingJob" />
        </xsd:sequence>
      </xsd:complexType>
      <xsd:element name="StartExecuteDosingJobListAsyncResponse">
        <xsd:annotation>
          <xsd:documentation>The response to the IDosingAutomationService.StartExecuteDosingJobListAsync request.</xsd:documentation>
        </xsd:annotation>
